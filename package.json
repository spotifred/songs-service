  "repository": {
    "type": "git",
    "url": "git+https://github.com/spotifred/spotifred.git"
  },
  "keywords": ["spotifred"],
  "author": "spotifred-team",
  "license": "MIT",
  "dependencies": {
    "Faker": "^0.7.2",
    "aws-sdk": "^2.192.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "concat-files": "^0.1.1",
    "csv-writer": "0.0.3",
    "dotenv": "^5.0.0",
    "fs": "0.0.1-security",
    "koa": "^2.4.1",
    "koa-body": "^2.5.0",
    "koa-morgan": "^1.0.1",
    "koa-response-time": "^2.0.0",
    "koa-router": "^7.4.0",
    "mocha": "^5.0.0",
    "newrelic": "^2.7.0",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "siege": "^0.2.0",
    "sqs-consumer": "^3.8.0"
  },
  "bugs": {
    "url": "https://github.com/spotifred/spotifred/issues"
  },
  "homepage": "https://github.com/spotifred/spotifred#readme",
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.5",
    "babel-register": "^6.26.0",
    "nodemon": "^1.14.12",
    "nyc": "^11.4.1",
    "why-is-node-running": "^1.2.3"
  },
  "nyc": {
    "include": ["database/**/*.js", "server/**/*.js"],
    "exclude": ["**/test/**/*", "packages/**/*"],
    "sourceMap": false,
    "instrument": false,
    "all": false
  }
